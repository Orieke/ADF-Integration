{
	"name": "Copy Churn Data",
	"properties": {
		"description": "Copy churn data from a blob storage to ADLS gen 2",
		"activities": [
			{
				"name": "Blob-ADLS2-Churn",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ChurnDataRaw",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ChurnDataADLS2",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "FileNameLookup",
				"type": "Lookup",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"dataset": {
						"referenceName": "ChurnDataRaw",
						"type": "DatasetReference"
					}
				}
			}
		],
		"parameters": {
			"FileName": {
				"type": "Array",
				"defaultValue": [
					"customer_demograhics.csv",
					"customer_transaction.csv"
				]
			}
		}
	}
}