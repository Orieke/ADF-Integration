{
	"name": "Copy Churn Data",
	"properties": {
		"description": "Copy churn data from a blob storage to ADLS gen 2",
		"activities": [
			{
				"name": "Blob-ADLS2-Churn",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "FileLoop",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "ChurnDataRaw",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ChurnDataADLS2",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "FileLoop",
				"type": "ForEach",
				"typeProperties": {
					"items": {
						"value": "{\n\"FileName\": \"customer_demographics.csv\"\n}\n{\n\"FileName\": \"customer_transaction.csv\"\n}",
						"type": "Expression"
					},
					"isSequential": true
				}
			}
		]
	}
}